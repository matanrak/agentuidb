{
  "name": "@agentuidb/core",
  "version": "1.0.0",
  "description": "Core engine: DB, tools, types for AgentUIDB",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./query": {
      "types": "./dist/sql.d.ts",
      "default": "./dist/sql.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./db": {
      "types": "./dist/db.d.ts",
      "default": "./dist/db.js"
    }
  },
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.0.0"
  }
}
