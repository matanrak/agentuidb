{
  "name": "agentuidb",
  "version": "1.0.0",
  "description": "MCP server providing structured data storage backed by SurrealDB",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "agentuidb": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "node scripts/test.mjs",
    "chat": "node scripts/chat.mjs",
    "watch": "node scripts/watch.mjs",
    "seed": "node scripts/seed.mjs",
    "db": "bash scripts/start-db.sh",
    "db:demo": "bash scripts/start-db-demo.sh",
    "db:load": "bash scripts/load-seed-data.sh"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.26",
    "@modelcontextprotocol/sdk": "^1.12.0",
    "surrealdb": "^1.3.2",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  }
}
